<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.chapman.cpsc356.spendingfriend">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.AccountActivity"
            android:parentActivityName=".activities.AccountListActivity" />
        <activity
            android:name=".activities.AccountListActivity"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.TransactionActivity"
            android:parentActivityName=".activities.TransactionListActivity" />
        <activity
            android:name=".activities.TransactionListActivity"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.BudgetActivity"
            android:parentActivityName=".activities.BudgetListActivity" />
        <activity
            android:name=".activities.SummaryActivity"
            android:parentActivityName=".activities.MainActivity" />
        <activity android:name=".activities.BudgetListActivity"
            android:parentActivityName=".activities.MainActivity"></activity>
    </application>

</manifest>